
@{
    ViewBag.Title = "Index2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Multiple Static Record</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>
            Name
        </th>
        <th>
            Contact Number
        </th>
    </tr>
    @foreach (var item in ViewBag.info)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Contact
            </td>
        </tr>
    }
</table>

